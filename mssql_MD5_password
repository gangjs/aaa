USE mabinogi
INSERT INTO account
VALUES (
    '계정명',
    CONVERT(NVARCHAR(32), HASHBYTES('MD5', '비밀번호'), 2),
    '이름',
    '1111-1111',
    'email@domain.com',
    '-1',
    '2017-01-22T17:10:24.3263939-08:00',
    '0',
    '(일반 계정 : 0 / npc : 5 / GM 계정 : 8)',
    '0','0','0','0','0','0','0'
);

비밀번호 변경
UPDATE dbo.account
SET password = CONVERT(VARCHAR(32), HASHBYTES('MD5', '0012345'), 2)
WHERE id = 'npc1';
